<div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <div class="card">
          <div class="card-header">
            <h3 class="card-title">FAQ Manager</h3>
          </div>
          <!-- /.card-header -->
          <div class="card-body">
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th style="width: 10px">#</th>
                  <th>Question</th>
                  <th>Answer</th>
                  <th style="width: 500px;">Created Date</th>
                  <th style="width: 40px">Status</th>
                  <th style="width: 150px">Action</th>
                </tr>
              </thead>
              <tbody>
                <% data.forEach(function(faq,i){  %>
                  <tr>
                    <th scope="row"><%= i+1 %></th>
                    <td><%= faq.question %> </td>
                    <td><%= faq.answer %> </td>
                    <td><%= faq.createdAt %> </td>
                    <td><%= faq.status %> </td>                    
                    <td>
                      <a class="btn bg-gradient-primary btn-sm">Edit</a>
                      <a class="btn bg-gradient-danger btn-sm">Delete</a>
                    </td>                    
                  </tr>
                <%  }) %>                
              </tbody>
            </table>
          </div>
          <!-- /.card-body -->
